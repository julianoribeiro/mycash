<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<contextListener class="ch.qos.logback.classic.jul.LevelChangePropagator" />
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d [%t] %-5p %c - %m%n</pattern>
		</encoder>
	</appender>
	<logger name="org.hibernate.impl.SessionFactoryObjectFactory" level="ERROR" />
	<logger name="org.springframework" level="WARN" />
	<logger name="org.primefaces" level="WARN" />
	<logger name="com.mysema" level="WARN" />
	<logger name="org.apache.http.wire" level="WARN" />
	<logger name="org.apache.catalina" level="WARN" />
	
	<logger name="org.hibernate" level="WARN" />
	
	<logger name="org.apache.commons.dbcp" level="INFO" />
	<logger name="net.sf.ehcache" level="INFO" />
	
	<logger name="br.com.vertexsoft" level="INFO" />
	
	<root level="WARN">
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>